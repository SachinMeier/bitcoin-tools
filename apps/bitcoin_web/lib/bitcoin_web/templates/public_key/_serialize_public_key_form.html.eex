<%= form_for @conn, @action, fn f -> %>

	<%= label f, "X coordinate (hex or integer):" %>
	<%= text_input f, :x %>
	<%= error_tag f, :x %>

	<%= label f, "Y coordinate (hex or integer):" %>
	<%= text_input f, :y %>
	<%= error_tag f, :y %>

	<div>
		<%= submit "serialize" %>
	</div>
<% end %>
